<%- include('st_header.ejs') %>

<h2> 과목코드 : <%= course.course_number %></h2>
<h2> 수업명 : <%= course.title %></h2>
<h3> 수업내용 : <%- course.memo.replace(/\n/g, '<br>') %></h3>

<hr>

<h3> 공지사항 </h3>


<table border=1>
   <tr>
       <th>idx</th>
       <th>제목</th>
       <th>내용</th>
   </tr>

   <% notices.forEach(function(notice) { %>
       <tr>
           <td><%= notice.idx %></td>
           <td><%= notice.title %></td>
           <td><%= notice.memo %></td>
       </tr>
   <% }) %>
</table>